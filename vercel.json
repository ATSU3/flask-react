{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/increment/(.*)",
            "dest": "/increment.py"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}